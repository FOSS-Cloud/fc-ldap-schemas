objectclass ( sstObjectClass:1
    NAME 'sstReseller'
    SUP top STRUCTURAL
    MUST ( uid $ organizationName ) 
    MAY ( sstIsActive ) )

objectclass ( sstObjectClass:2
    NAME 'sstAddressCompany'
    SUP top AUXILIARY
    MUST ( countryName $ givenName $ localityName $ mail $ organizationName $ 
           postalCode $ preferredLanguage $ sstGender $ surname )
    MAY ( mobileTelephoneNumber $ postalAddress $ postOfficeBox $
          sstWebsiteURL $ stateOrProvinceName $ telephoneNumber ) )

objectclass ( sstObjectClass:3
    NAME 'sstAddressPerson'
    SUP top AUXILIARY
    MUST ( countryName $ givenName $ localityName $ mail $ postalCode $
           preferredLanguage $ sstGender $ surname )
    MAY ( mobileTelephoneNumber $ postalAddress $ postOfficeBox $
          sstWebsiteURL $ stateOrProvinceName $ telephoneNumber ) )

objectclass ( sstObjectClass:4
    NAME 'sstBillingReseller'
    SUP top AUXILIARY
    MUST ( sstResellerBill )
    MAY ( sstBillingCycle $ sstCurrency $ sstPaymentMethod $ 
          sstResellerDiscountFormula $ sstResellerMinimalDiscount ) )

objectclass ( sstObjectClass:5
    NAME 'sstBillingCustomer'
    SUP top AUXILIARY
    MUST ( sstBillingCycle $ sstBillingCycleFix $ sstCurrency $ 
           sstPaymentMethod ) )

objectclass ( sstObjectClass:6
    NAME 'sstPerson'
    SUP top STRUCTURAL
    MUST ( givenName $ mail $ preferredLanguage $ sstBelongsToCustomerUID $
           sstTimeZoneOffset $ surname $ uid $ userPassword )
    MAY ( cn $ description $ mobileTelephoneNumber $ sstBelongsToResellerUID $ sstBelongsToUID $ 
          telephoneNumber $ sstGroupUID $ sstRoleUID $ sstLDAPForeignStaticAttribute $ 
		  sstGroupName $ sstIsActive $ sstGender ) )

objectclass ( sstObjectClass:7
    NAME 'sstCustomerCompany'
    SUP top STRUCTURAL
    MUST ( organizationName $ sstBelongsToResellerUID $ uid )
    MAY ( sstIsActive ) )

objectclass ( sstObjectClass:8
    NAME 'sstCustomerPerson'
    SUP top STRUCTURAL
    MUST ( givenName $ surname $ sstBelongsToResellerUID $ uid )
    MAY ( sstIsActive ) )

objectclass ( sstObjectClass:9
    NAME 'sstBillingProduct'
    SUP top STRUCTURAL
    MUST ( sstBillable $ sstBilledFrom $ sstBilledUntil $ 
           sstBillingPriceSchemaCustomer $ sstBillingPriceSchemaReseller $
           sstBillingStartDate $ sstCancellationNotice $ sstProductDescription $
	   sstProductName )
    MAY ( ou $ sstQuota $ uid ) )

objectclass ( sstObjectClass:10
    NAME 'sstBillingProductItem'
    SUP top STRUCTURAL
    MUST ( sstBillable $ sstBilledFrom $ sstBilledUntil $
           sstBillingPriceSchemaCustomer $ sstBillingPriceSchemaReseller $
           sstBillingStartDate $ sstProductDescription $ sstProductName )
    MAY ( ou $ sstDiskspace $ sstMemory $ sstQuota $ uid ) )

objectclass ( sstObjectClass:11
    NAME 'sstProvisioning'
    SUP top AUXILIARY
    MUST ( sstProvisioningExecutionDate $ sstProvisioningMode $ 
           sstProvisioningState)
    MAY ( sstProvisioningReturnValue $ sstProvisioningExecutionTime ) )

objectclass ( sstObjectClass:12
    NAME 'sstBillingPriceSchema'
    SUP top STRUCTURAL
    MUST ( description $ sstBasePrice $ sstBelongsToUID $ sstBillingUnit $ sstFreeUnit $ 
           sstPriceFormula $ sstPricePerUnit $ sstProductName $ uid ) )

objectclass ( sstObjectClass:13
    NAME 'sstMailDomain'
    SUP top STRUCTURAL
    MUST ( sstIsActive $ sstMailDomain )
    MAY ( sstBelongsToUID ) )

objectclass ( sstObjectClass:14
    NAME 'sstMailAccount'
    SUP top STRUCTURAL
    MUST ( givenName $ preferredLanguage $ sstIsActive $ sstGender $ 
           sstMail $ sstMailMessageHomeDirectory $ sstMailMessageStore $ 
	   sstMailReadIMAP $ sstMailReadPOP $ sstMailReceive $ sstMailSend $ 
	   sstMailWebmail $ sstQuota $ surname $ userPassword )
    MAY ( sstBelongsToUID $ sstGroupware $ sstGroupwareOXAccountType $ sstGroupwareOXContextID $ 
          sstMailAccountFolderConfirmedHam $ sstMailAccountFolderConfirmedSpam $ 
	  sstMailAccountFolderDrafts $ sstMailAccountFolderSent $ sstMailAccountFolderSpam 
	  $ sstMailAccountFolderTrash $ sstMailForward ) )

objectclass ( sstObjectClass:15
    NAME 'sstMailAlias'
    SUP top STRUCTURAL
